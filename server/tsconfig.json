{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "../",
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}
